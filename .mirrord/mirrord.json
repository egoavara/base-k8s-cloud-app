{
    "target": "targetless",
    "agent": {
        "namespace": "service"
    },
    "feature": {
        "network": {
            "outgoing": {
                "tcp": true,
                "udp": true
            },
            "dns": true
        },
        "fs": "read",
        "env": true
    },
    "kubeconfig": "~/.kube/config",
    "kube_context": "kane-large-context",
    "accept_invalid_certificates": true
}